<div class="panel panel-default">
  <div class="panel-heading">
     <h3 class="panel-title">Property Details</h3>
  </div>
  <div class="panel-body">

    <div class="form-group">
      <%= f.label :SELLER %>
      <%= f.text_field :SELLER, class: 'col-sm-9' %>
    </div>

    <div class="form-group">
      <%= f.label :CLOSE_DATE, "Closing Date" %>
      <%= f.date_field :CLOSE_DATE %>
    </div>

    <div class="form-group">
      <%= f.label :ADDRESS %>
      <%= f.text_field :ADDRESS, class: 'col-sm-9' %>
    </div>

    <div class="form-group">
      <%= f.label :TOWN %>
      <%= f.text_field :TOWN, class: 'col-sm-9' %>
    </div>

    <div class="form-group">
      <%= f.label :COUNTY %>
      <%= f.text_field :COUNTY, class: 'col-sm-9' %>
    </div>

    <div class="form-group">
      <%= f.label :PARCEL_NO, "Parcel Number" %>
      <%= f.text_field :PARCEL_NO, class: 'col-sm-9' %>
    </div>

    <div class="form-group">
      <%= f.label :PROP_CODE, "PROP CODE" %>
      <%= f.text_field :PROP_CODE, class: 'col-sm-9' %>
    </div>
  </div>
</div>
