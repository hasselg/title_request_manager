<div class="container">
  <div class="row">
    <%= form_for @underwriter do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>

      <div class="form-group">
        <%= f.label :abbreviation %>
        <%= f.text_field :abbreviation %>
      </div>

      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
